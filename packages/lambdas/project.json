{
  "name": "lambdas",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/lambdas/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/lambdas",
        "main": "packages/lambdas/src/index.ts",
        "tsConfig": "packages/lambdas/tsconfig.json",
        "assets": ["packages/lambdas/*.md"],
        "format": ["cjs"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/lambdas/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/lambdas/jest.config.ts"
      }
    }
  },
  "tags": []
}
